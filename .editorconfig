root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = space
insert_final_newline = true
max_line_length = 120
trim_trailing_whitespace = true

[**/*.css]
max_line_length = 180
print_width = 180

[**/*.scss]
max_line_length = 180
print_width = 180

[**/*.java]
max_line_length = 140
print_width = 140

[**/*.xml]
max_line_length = 180
print_width = 180

[**/*.properties]
max_line_length = 360
print_width = 360
